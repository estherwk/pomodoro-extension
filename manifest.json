{
  "manifest_version": 3,
  "name": "Custom Pomodoro Timer",
  "version": "1.0",
  "description": "A Pomodoro Timer with customizable background image and sound",
  "permissions": [
    "storage",
    "notifications"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}